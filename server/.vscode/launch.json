{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to server gdb",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/app.remote",
      "MIMode": "gdb",
      "cwd": "${workspaceFolder}",
      "miDebuggerServerAddress": "localhost:7777",
      "miDebuggerPath": "gdb",
      "useExtendedRemote": true,
      "stopAtEntry": false,
      "preLaunchTask": "copy-remote-binary",
      "sourceFileMap": {
        "/build/openssl": "${workspaceFolder}/../openssl"
      },
      "setupCommands": [
        {
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "text": "set remote exec-file /src/app",
          "ignoreFailures": false
        },
        {
          "text": "set substitute-path /build/openssl ${workspaceFolder}/../openssl",
          "ignoreFailures": true
        }
      ],
      "logging": {
        "engineLogging": true
      }
    }
  ]
}